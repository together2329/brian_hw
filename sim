#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x1126040a0 .scope module, "counter_tb" "counter_tb" 2 3;
 .timescale -9 -12;
v0x112614700_0 .var "clk", 0 0;
v0x1126147a0_0 .net "count", 3 0, v0x112614550_0;  1 drivers
v0x112614850_0 .var "rst_n", 0 0;
S_0x112604220 .scope module, "uut" "counter" 2 9, 3 1 0, S_0x1126040a0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 4 "count";
v0x1126044a0_0 .net "clk", 0 0, v0x112614700_0;  1 drivers
v0x112614550_0 .var "count", 3 0;
v0x112614600_0 .net "rst_n", 0 0, v0x112614850_0;  1 drivers
E_0x112604440/0 .event negedge, v0x112614600_0;
E_0x112604440/1 .event posedge, v0x1126044a0_0;
E_0x112604440 .event/or E_0x112604440/0, E_0x112604440/1;
    .scope S_0x112604220;
T_0 ;
    %wait E_0x112604440;
    %load/vec4 v0x112614600_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x112614550_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x112614550_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x112614550_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1126040a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x112614700_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x112614700_0;
    %inv;
    %store/vec4 v0x112614700_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x1126040a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x112614850_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x112614850_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 27 "$display", "Simulation complete" {0 0 0};
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x1126040a0;
T_3 ;
    %vpi_call 2 33 "$monitor", "Time = %0t, rst_n = %b, count = %b (%0d)", $time, v0x112614850_0, v0x1126147a0_0, v0x1126147a0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
