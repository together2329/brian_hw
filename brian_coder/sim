S_0x12a604080 .scope module, "counter_4bit_tb" "counter_4bit_tb" 2 1;
 .timescale 0 0;
v0x6000038e01b0_0 .var "clk", 0 0;
v0x6000038e0240_0 .net "count", 3 0, v0x6000038e0090_0;  1 drivers
v0x6000038e02d0_0 .var "reset", 0 0;
S_0x12a6041f0 .scope module, "uut" "counter_4bit" 2 9, 3 1 0, S_0x12a604080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x6000038e0000_0 .net "clk", 0 0, v0x6000038e01b0_0;  1 drivers
v0x6000038e0090_0 .var "count", 3 0;
v0x6000038e0120_0 .net "reset", 0 0, v0x6000038e02d0_0;  1 drivers
E_0x600001fe8000 .event posedge, v0x6000038e0000_0;
    .scope S_0x12a6041f0;
T_0 ;
    %wait E_0x600001fe8000;
    %load/vec4 v0x6000038e0120_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x6000038e0090_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6000038e0090_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x6000038e0090_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x12a604080;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x6000038e01b0_0;
    %inv;
    %store/vec4 v0x6000038e01b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12a604080;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000038e01b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6000038e02d0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000038e02d0_0, 0, 1;
    %delay 160, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x12a604080;
T_3 ;
    %wait E_0x600001fe8000;
    %vpi_call 2 37 "$display", "Time: %0t | clk: %b | reset: %b | count: %b", $time, v0x6000038e01b0_0, v0x6000038e02d0_0, v0x6000038e0240_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
